    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "playwright": "^1.40.0"
  },
  "files": [
    "dist",
    "README.md",
